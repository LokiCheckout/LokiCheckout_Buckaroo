<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="LokiCheckout\Core\Config\LokiCheckoutModules">
        <arguments>
            <argument name="modules" xsi:type="array">
                <item name="LokiCheckout_Buckaroo" xsi:type="string">LokiCheckout_Buckaroo</item>
            </argument>
        </arguments>
    </type>

    <type name="LokiCheckout\Buckaroo\Service\TokenService">
        <arguments>
            <argument name="configProviderCreditcard" xsi:type="object">Buckaroo\Magento2\Model\ConfigProvider\Method\Creditcards\Proxy</argument>
        </arguments>
    </type>

    <type name="LokiCheckout\Buckaroo\Payment\Icon\IconResolver">
        <arguments>
            <argument name="infoBlock" xsi:type="object">Buckaroo\Magento2\Block\Info\Proxy</argument>
        </arguments>
    </type>
</config>
